@model LeerplatformJH.Models.ViewModels.StudentInschrijvingen

<!DOCTYPE html>
<html>
<body>

    <h1>Student Inschrijvingen</h1>

    <p>Student Naam: @Model.Student.Voornaam @Model.Student.Achternaam</p>

    <table>
        <thead>
            <tr>
                <th>Vak</th>
                <th>Goedkeuring</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var vakInschrijving in Model.VakInschrijvingen)
            {
                <tr>
                    <td>@vakInschrijving.Vak.Titel</td>
                    <td>@vakInschrijving.Goedkeuring</td>
                </tr>
            }
        </tbody>
    </table>

</body>
</html>